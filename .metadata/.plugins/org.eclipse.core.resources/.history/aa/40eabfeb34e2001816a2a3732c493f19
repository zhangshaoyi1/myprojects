package entity;

public class chacters {
	private String chacter;		//章节名
	private String text;		//章节内容
	private String prev;		//上一章节地址
	private String next;		//下一张地址
	public String getChacter() {
		return chacter;
	}
	public void setChacter(String chacter) {
		this.chacter = chacter;
	}
	public String getText() {
		return text;
	}
	public void setText(String text) {
		this.text = text;
	}
	public String getPrev() {
		return prev;
	}
	public void setPrev(String prev) {
		this.prev = prev;
	}
	public String getNext() {
		return next;
	}
	public void setNext(String next) {
		this.next = next;
	}
	
}
